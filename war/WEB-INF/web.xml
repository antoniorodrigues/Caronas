<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	
	<welcome-file-list>
		<welcome-file>ImageViewer.html</welcome-file>
	</welcome-file-list>
	
	 <!-- Servlets -->
	 <servlet>
        <servlet-name>parceirosDaEstradaService</servlet-name>
        <servlet-class>parceirosDaEstrada.web.server.ControleDeAcessoImpl</servlet-class>
    </servlet>
	
    <servlet-mapping>
        <servlet-name>parceirosDaEstradaService</servlet-name>
        <url-pattern>/parceirosDaEstrada.web.ImageViewer/parceirosService</url-pattern>
    </servlet-mapping>    
    
  <inherits name="com.google.gwt.http.HTTP" />

</web-app>
